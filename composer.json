{
  "name": "ixbox/phambda",
  "description": "",
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^8.1",
    "php-http/discovery": "^1.17",
    "psr/container": "^2.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Phambda\\": "src"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.52",
    "guzzlehttp/guzzle": "^7.8",
    "guzzlehttp/psr7": "^2.6"
  },
  "scripts": {
    "analyze": [
      "php vendor/bin/phpstan.phar analyse --level max src"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
